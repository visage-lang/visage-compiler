                <arg line="${test.class}"/>
            </customize>
        </javafxproject3:debug>
    </target>
    <target depends="init,compile-test" if="netbeans.home+have.tests" name="-debug-start-debugger-test">
        <javafxproject1:nbjpdastart classpath="${debug.test.classpath}" name="${test.class}"/>
